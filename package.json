{
  "name": "fastify_backend_template",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build:ts": "tsc",
    "start": "npm run build:ts && node dist/server.js",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"Fastify\" -c \"green.bold,cyan.bold\"  \"env-cmd ./config/development.env nodemon\"",
    "test": "npm run build:ts && tsc -p test/tsconfig.test.json && tap test/**/*.test.ts",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prettier": "prettier **/*.{js,ts} --check",
    "prettier:fix": "npm run prettier -- --write",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.9.1",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "pino-pretty": "^4.7.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/tap": "^14.10.1",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^8.0.2",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "tap": "^14.11.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  }
}
